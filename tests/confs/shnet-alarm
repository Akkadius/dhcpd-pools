shared-network example1 {
	subnet 10.0.0.0  netmask 255.255.255.0 {
		pool {
			range 10.0.0.1 10.0.0.20;
		}
	}
	subnet 10.1.0.0  netmask 255.255.255.0 {
		pool {
			range 10.1.0.1 10.1.0.20;
		}
	}
}

